<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <inherits name="lando.systems.ld50.GdxDefinition" />
    <inherits name='com.kotcrab.vis.vis-ui' />

    <inherits name="formic" />
    <inherits name="regexodus" />
    <inherits name="space.earlygrey.shapedrawer" />
    <inherits name="com.kotcrab.vis.vis-ui" />
    <inherits name="aurelienribon.tweenengine" />

    <extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld50.particles.Particle" />
    <extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld50.utils.typinglabel.TypingGlyph" />
    <extend-configuration-property name="gdx.reflect.include" value="lando.systems.ld50.utils.typinglabel.effects" />

    <collapse-all-properties />
    <add-linker name="xsiframe"/>
    <set-configuration-property name="devModeRedirectEnabled" value="true"/>
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
</module>